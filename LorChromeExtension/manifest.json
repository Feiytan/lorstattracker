{
    "manifest_version": 2,
    "name": "LOR StatTracker",
    "version": "1.0",
    "description": "An extension to track you games of Legends Of Runeterra.",
    "browser_action": {
        "default_popup": "popup/index.html"
    },
    "permissions": [
        "http://127.0.0.1:21337/*",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistant": false
    },
    "content_scripts": [
    {
        "js": ["content.js"],
        "matches": ["http://localhost:4200/*"]
    }]
}